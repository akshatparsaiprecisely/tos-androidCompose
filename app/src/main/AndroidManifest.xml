<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.tos_androidcompose">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TosandroidCompose"
        tools:targetApi="31">
        <activity
            android:name=".ui.FormDashboardScreen.FormDataDashboardActivity"
            android:exported="false"
            android:label="@string/title_activity_form_data_dashboard"
            android:theme="@style/Theme.TosandroidCompose" />
        <activity
            android:name=".ui.formStatisticsScreen.FormStatisticsScreen"
            android:exported="false"
            android:label="@string/title_activity_form_statistics_screen"
            android:theme="@style/Theme.TosandroidCompose" />
        <activity
            android:name=".ui.aboutScreen.AboutScreen"
            android:exported="false"
            android:label="@string/title_activity_about_screen"
            android:theme="@style/Theme.TosandroidCompose" />
        <activity
            android:name=".ui.settingsScreen.SettingsScreen"
            android:exported="false"
            android:label="@string/title_activity_settings_screen"
            android:theme="@style/Theme.TosandroidCompose" />
        <activity
            android:name=".ui.login.LoginScreen"
            android:exported="true"
            android:label="@string/title_activity_login_screen"
            android:theme="@style/Theme.TosandroidCompose">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.notifiction.NotificationScreen"
            android:exported="false"
            android:label="@string/title_activity_notification_screen"
            android:theme="@style/Theme.TosandroidCompose" />
        <activity
            android:name=".ui.projects.ProjectScreen"
            android:exported="false"
            android:label="@string/title_activity_project_screen"
            android:theme="@style/Theme.TosandroidCompose" />
        <activity
            android:name=".ui.chats.ChatScreen"
            android:exported="false"
            android:label="@string/title_activity_chat_screen"
            android:theme="@style/Theme.TosandroidCompose" />
        <activity
            android:name=".ui.dashboard.DashboardActivity"
            android:exported="false"
            android:theme="@style/Theme.TosandroidCompose" />
    </application>

</manifest>